from graphics import *
def main():
    #draw the canvas
    win=GraphWin("Rectangle")

    #draw the rectangle
    rect=Rectangle(Point(30,30),Point(110,50))
    rect.setFill('pink')

    #draw the circle
    center=Point(105,50)
    circ2=Circle(center,15)
    circ2.setFill('red')

    center=Point(35,50)
    circ=Circle(center,15)
    circ.setFill('red')
    
    #draw the shapes on the canvas
    circ2.draw(win)
    circ.draw(win)
    rect.draw(win)
main()
